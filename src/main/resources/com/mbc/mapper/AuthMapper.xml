<mapper namespace="com.mbc.mapper.AuthMapper">
    
    <!-- 권한 추가 SQL -->
    <insert id="insertAuth" parameterType="com.mbc.domain.AuthVO">
        INSERT INTO users_auth (uno, auth)
        VALUES (#{uno}, #{auth})
    </insert>

</mapper>